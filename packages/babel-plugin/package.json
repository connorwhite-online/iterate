{
  "name": "@iterate/babel-plugin",
  "version": "0.1.0",
  "description": "Babel plugin that injects React component names and source locations for iterate",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/babel__core": "^7.20.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  }
}
