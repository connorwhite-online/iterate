{
  "name": "iterate-ui-vite",
  "version": "0.1.0",
  "description": "iterate Vite plugin â€” auto-starts daemon and injects overlay",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --external @babel/core",
    "dev": "tsup src/index.ts --format esm --dts --watch --external @babel/core",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "iterate-ui-babel-plugin": "workspace:*",
    "iterate-ui-overlay": "workspace:*"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@types/babel__core": "^7.20.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
