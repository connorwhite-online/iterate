{
  "name": "iterate-ui-next",
  "version": "0.1.0",
  "description": "iterate Next.js plugin â€” auto-starts daemon and injects overlay",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts",
    "dev": "tsup src/index.ts --format esm --dts --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "iterate-ui-daemon": "workspace:*",
    "iterate-ui-overlay": "workspace:*",
    "iterate-ui-babel-plugin": "workspace:*"
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  }
}
